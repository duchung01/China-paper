#include<bits/stdc++.h>
using namespace std;

const int N = 1e6 + 5;

int n;
int a[2 * N] , suma[2 * N] , p[N] , d[N];
bool check[N];
deque<int> q;

void solve(int type)
{
    for (int i = 1 ; i < n ; ++i) a[i + n] = a[i];
    for (int i = 1 ; i <= 2 * n - 1 ; ++i) suma[i] = suma[i - 1] + a[i];

    q.clear();
    for (int i = 1 ; i < n ; ++i)
    {
        while(!q.empty() && suma[q.back()] >= suma[i])
        {
            q.pop_back();
        }
        q.push_back(i);
    }

    for (int i = 1 ; i <= n ; ++i)
    {
        if (!q.empty() && q.front() == i - 1) q.pop_front();

        while(!q.empty() && suma[q.back()] >= suma[i + n - 1])
        {
            q.pop_back();
        }

        q.push_back(i + n - 1);
        if (suma[q.front()] >= suma[i - 1])
        {
            if (type == 1)
            {
                check[i] = true;
            }
            else
            {
                // do a[1] bieu dien cho viec di tu tram n - i + 1 den tram n - i nen la check[n - i + 1] = true
                check[n - i + 1] = true;
            }
        }
    }
}

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin >> n;
    for (int i = 1 ; i <= n ; ++i)
    {
        cin >> p[i] >>  d[i];
        a[i] = p[i] - d[i];
    }

    solve(1);

    // tao mot mang moi cho duong theo chieu nguoc kim dong ho
    for (int i = n ; i >= 1 ; --i)
    {
        int nxt = i - 1;
        if (nxt < 1) nxt += n;
        // a[n - i + 1] la chenh lech nhien lieu voi duong di tu tram i den i - 1
        a[n - i + 1] = p[i] - d[nxt];
    }

    // modify nhu vay giup chung ta co the dung lai ham solve
    solve(2);

    for (int i = 1 ; i <= n ; ++i)
    {
        if (check[i] == true)
        {
            cout << "TAK";
        }
        else
        {
            cout << "NIE";
        }
        cout << "\n";
    }

}
